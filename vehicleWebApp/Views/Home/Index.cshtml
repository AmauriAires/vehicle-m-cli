@{
    ViewData["Title"] = "Home Page";
}

<br />
<div class="row">
    <div class="col-md-2">
        <label for="validationCustom01">Name:</label>
        <select class="form-control form-control-sm" name="category" id="validationCustom01" onchange="ChangecatList()" required>
            <option selected="selected" value="Civic">Civic</option>
            <option value="Cruze">Cruze</option>
            <!--
            <option value="Elantra">Elantra</option>
            <option value="Gol">Gol</option>
            <option value="Camry">Camry</option>
            <option value="Accord">Accord</option>
            <option value="Focus">Focus</option>
            <option value="Fusion">Fusion</option>
            -->
        </select>
        <div class="invalid-feedback">
            Please provide a name.
        </div>
    </div>
    <div class="col-md-3">
        <label for="validationCustom02">Brand:</label>
        <select class="form-control form-control-sm" name="category" id="validationCustom02" onchange="ChangecatList2()" required>
            <option selected="selected" value="Honda">Honda</option>
            <option value="Chevrolet">Chevrolet</option>
            <!--
            <option value="Hyundai">Hyundai</option>
            <option value="Volkswagen">Volkswagen</option>
            <option value="Toyota">Toyota</option>
            <option value="Ford">Ford</option>
            -->
        </select>
        <div class="invalid-feedback">
            Please provide a model.
        </div>
    </div>
    <div class="col-md-3">
        <label for="validationCustom03">Color:</label>
        <select class="form-control form-control-sm" name="category" id="validationCustom03" onchange="ChangecatList3()" required>
            <option value="Blue">Blue</option>
            <option value="Red">Red</option>
            <!--
            <option value="Yellow">Yellow</option>
            <option value="Purple">Purple</option>
            <option value="Orange">Orange</option>
            <option value="Green">Green</option>
            <option selected="selected" value="Gray">Gray</option>
            <option value="Black">Black</option>
            -->
        </select>
        <div class="invalid-feedback">
            Please provide a color.
        </div>
    </div>
    <div class="col-md-3">
        <label for="validationCustom04">Year:</label>
        <select class="form-control form-control-sm" name="category" id="validationCustom04" onchange="ChangecatList4()" required>
            <option value="2018">2018</option>
            <option value="2017">2017</option>
            <option selected="selected" value="2016">2016</option>
            <!--
            <option value="2015">2015</option>
            <option value="2014">2014</option>
            <option value="2013">2013</option>
            <option value="2012">2012</option>
            <option value="2011">2011</option>
            -->
        </select>
        <div class="invalid-feedback">
            Please provide a year.
        </div>
    </div>
    <div class="col-md-1">
        <label for="validationCustom05">...</label>
        <input type="submit" value="submit" class="btn btn-primary" id="submitBtn" />
    </div>
</div>
